@model List<LichLamViecModel>
@{
    ViewData["Title"] = "Lịch làm việc";
    Layout = "~/Views/Shared/MasterLayout.cshtml";
}
<style>
    thead th, tbody td {
        text-align: center;
    }
    .file{
        background: red;
        color: #fff;
    }
    .tab-content > .active {
        opacity: 1;
    }
    li.active a.nav-link {
        font-weight: bold;
        background-color: #f5811f !important;
        color: #fff !important;
        margin-right: 0!important;
        border: 0!important;
    }
    .nav-tabs > li.nav-item {
        box-shadow: 0px 0px 2px #adacac;
    }
    .nav-tabs > li.nav-item a {
        color: #f5811f;
    }
    li.nav-item:last-child {
        border-radius: 0 4px 0 0;
    }
    li.nav-item:first-child {
        border-radius: 4px 0 0 0;
    }
</style>
<title>@ViewData["Title"]</title>
<h2>Lịch làm việc</h2>

<div class="row">
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item active">
            <a class="nav-link" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Phòng vé</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Kế toán</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Kinh doanh</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="IT-tab" data-toggle="tab" href="#IT" role="tab" aria-controls="IT" aria-selected="false">IT</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="dulich-tab" data-toggle="tab" href="#dulich" role="tab" aria-controls="dulich" aria-selected="false">Du lịch</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="khac-tab" data-toggle="tab" href="#khac" role="tab" aria-controls="khac" aria-selected="false">Khác</a>
        </li>
    </ul>
