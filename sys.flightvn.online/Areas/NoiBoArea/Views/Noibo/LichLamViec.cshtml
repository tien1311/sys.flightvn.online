                        <tr>
                            <th style="vertical-align: middle;">STT</th>
                            <th style="vertical-align: middle;">Họ tên</th>
                            <th>@DateTime.Now.AddDays(-1).ToString("dd/MM/yyyy")<br/>@Model[0].ThuHwa</th>
                            <th class="info">@DateTime.Now.ToString("dd/MM/yyyy")<br />@Model[0].ThuNgay1</th>
                            <th>@DateTime.Now.AddDays(+1).ToString("dd/MM/yyyy")<br />@Model[0].ThuNgay2</th>
                            <th>@DateTime.Now.AddDays(+2).ToString("dd/MM/yyyy")<br />@Model[0].ThuNgay3</th>
                            <th>@DateTime.Now.AddDays(+3).ToString("dd/MM/yyyy")<br />@Model[0].ThuNgay4</th>
                            <th>@DateTime.Now.AddDays(+4).ToString("dd/MM/yyyy")<br />@Model[0].ThuNgay5</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{int i = 1; }
                        @foreach (var item in Model)
                        {
                            @if (item.PhongBan.ToUpper() == "PHÒNG VÉ" || item.PhongBan.ToUpper() == "CALL CENTER")
                            {
                                <tr>
                                    <td style=" width: 45px; vertical-align: middle">@i</td>
                                    <td style="width:300px; text-align:left;">
                                        @item.Ten <br />
                                        @item.DT&emsp;@{
                                            if (item.Line != null && item.Line != "")
                                            {
                                                string Line = "Ext:" + item.Line;
                                                @Line;
                                            }
                                        }
                                    </td>
                                    <td style="">
                                        @if (item.Gio == "False")
                                        {
                                            @item.GioHwa_1 <br /> @item.GioHwa_2 <br />
                                        }
                                        else { @item.Gio <br/><br /> }
                                        @if (item.DemFileHW != "0")
                                        {
                                            <a href="@item.FileHwa" target="_blank"><strong class="file">@item.HWA</strong></a>
                                        }
                                        else
                                        {<strong>@item.HWA</strong>}
                                    </td>
                                    <td class="info" style="">
                                        @if (item.Gio == "False")
                                        {
                                            @item.GioNgay1_1 <br /> @item.GioNgay1_2 <br />
                                        }
                                        else
